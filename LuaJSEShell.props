plugin.luaj.seshell.LuaJSEShellPlugin.name=LuaJ-SE Shell
plugin.luaj.seshell.LuaJSEShellPlugin.author=Zigmantas Kryzius
plugin.luaj.seshell.LuaJSEShellPlugin.description=\
		Provides an interactive LuaJ - Scripting Engine (LuaJ-SE) console and allows macros \
		and startup scripts to be written in LuaJ.
plugin.luaj.seshell.LuaJSEShellPlugin.version=1.0.0
plugin.luaj.seshell.LuaJSEShellPlugin.docs=docs/index.html
plugin.luaj.seshell.LuaJSEShellPlugin.activate=defer
#startup
plugin.luaj.seshell.LuaJSEShellPlugin.depend.0=jedit 05.03.00.00
plugin.luaj.seshell.LuaJSEShellPlugin.depend.1=jdk 1.8
plugin.luaj.seshell.LuaJSEShellPlugin.depend.2=plugin console.ConsolePlugin 4.3.3
plugin.luaj.seshell.LuaJSEShellPlugin.depend.3=plugin luaj.LuaJPlugin 1.0.0

macro-handler.LuaJSEHandler.label=LuaJSEHandler
macro-handler.LuaJSEHandler.glob=*.lua

luaj-error.title=LuaJ Error
luaj-error.message=A LuaJ error occurred.

console.shell.LuaJ-SE-show.label=LuaJ-SE Shell

luaj.seshell.info.toggle=true
luaj.seshell.info=LuaJ 3.0.1: scripting engine.

luaj.seshell.prompt=LuaJ=>

luaj.seshell.eval-selection.label=Evaluate selection in LuaJ-SE
luaj.seshell.eval-buffer.label=Evaluate buffer in LuaJ-SE
luaj.seshell.eval-script.label=Evaluate file in LuaJ-SE
luaj.seshell.print-selection.label=Print selection in LuaJ-SE
luaj.seshell.eval-startup.label=Load startup file(s)

plugin.luaj.seshell.LuaJSEShellPlugin.menu=console.shell.LuaJ-SE-show - \
	luaj.seshell.eval-selection \
	luaj.seshell.eval-buffer \
	luaj.seshell.eval-script \
	luaj.seshell.print-selection - \
	luaj.seshell.eval-startup

